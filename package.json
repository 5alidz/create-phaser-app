{
    "name": "create-phaser-app",
    "version": "0.0.2",
    "description":
        "A boiler plate to start your phaser app with webpack4, babel7+ and Phaser 3",
    "author": "Jesse Harlin",
    "homepage": "https://the-simian.github.io/create-phaser-app",
    "scripts": {
        "start": "webpack-dev-server --mode development --env.development",
        "build": "webpack --mode production --env.production",
        "deploy":
            "yarn build --env.ghpages && yarn gh-pages && rm -rf ./gh-pages",
        "test": "eslint ./src/**/**.js",
        "complexity-report": "./node_modules/.bin/es6-plato -r -d ./report src",
        "gh-pages": "gh-pages -d gh-pages"
    },
    "keywords": [
        "Phaser",
        "Phaser 3",
        "webpack",
        "webpack 4",
        "game development",
        "boilerplate",
        "scaffold",
        "prettier",
        "game jam"
    ],
    "license": "MIT",
    "dependencies": {
        "@babel/core": "7.0.0-beta.49",
        "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
        "@babel/plugin-proposal-decorators": "^7.0.0-beta.49",
        "@babel/preset-env": "^7.0.0-beta.49",
        "@babel/preset-react": "^7.0.0-beta.49",
        "@babel/runtime": "7.0.0-beta.48",
        "add": "^2.0.6",
        "babel-eslint": "^8.2.3",
        "babel-loader": "8.0.0-beta.0",
        "browser-sync": "^2.18.13",
        "browser-sync-webpack-plugin": "^2.2.2",
        "bs-eslint-message": "^0.0.3",
        "chalk": "^2.4.1",
        "clean-webpack-plugin": "^0.1.17",
        "copy-webpack-plugin": "^4.3.1",
        "css-loader": "^0.28.11",
        "es6-plato": "^1.0.18",
        "eslint": "^4.3.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.8.2",
        "expose-loader": "^0.7.3",
        "file-loader": "^1.1.11",
        "gh-pages": "^1.1.0",
        "html-webpack-plugin": "^3.2.0",
        "json-loader": "^0.5.7",
        "phaser": "3.9.0",
        "phaser-animated-tiles": "^1.0.0",
        "prettier": "^1.12.1",
        "prettier-eslint": "^8.8.1",
        "raw-loader": "^0.5.1",
        "style-loader": "^0.21.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.8.3",
        "webpack-bundle-analyzer": "^2.13.1",
        "webpack-cli": "^2.1.4",
        "webpack-dev-server": "^3.1.4",
        "yarn": "^1.7.0"
    }
}
